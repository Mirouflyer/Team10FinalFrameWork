<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Century21 Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="30.0"/>
    <parameter name="url" value="https://www.c21stores.com"/>

    <test name = "#Century21 Home">
        <classes>
             <class name="title.Century21Home"/>
            <class name="testAccount.TestRegistration"/>
            <class name="testAccount.TestLogIn"/>
            <class name="menu.MenuTest"/>
        </classes>
    </test>


</suite>